/* 
 * 默认带边框的盒子 (结构)
 * <div class="border-box">
 * 	  <div class="border-box-inner"></div>
 * </div>
 */
.border-box {
	margin: 0 auto;
	position: relative;
}
.border-box::before,
.border-box::after {
	content: '';
	position: absolute;
	right: 0;
	left: 0;
	height: 26rpx;
	background: url(data:image/png;base64,) no-repeat;
	background-size: 100%;
	z-index: 9;
}
.border-box::before {
	top: 0;
}
.border-box::after {
	bottom: 0;
	-webkit-transform: rotate(180deg);
	        transform: rotate(180deg);
}
.border-box-inner {
	position: relative;
	height: 100%;
}
.border-box-inner::before,
.border-box-inner::after {
	content: '';
	position: absolute;
	left: 0;
	top: 36rpx;
	bottom: 36rpx;
	width: 2rpx;
	background: url(data:image/png;base64,) repeat;
	background-size: 100%;
}
.border-box-inner::after {
	left: auto;
	right: 0;
	-webkit-transform: rotate(180deg);
	        transform: rotate(180deg);
}
/* 底部按钮统一样式 */
.fixed-footer {
	position: fixed;
	bottom: 0;
	right: 0;
	left: 0;
	height: 100rpx;
	line-height: 100rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.btn-footer {
