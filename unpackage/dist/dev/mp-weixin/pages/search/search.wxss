/* 面板相关字体 */
.search-page {
  padding: 20rpx 30rpx;
}
.search-page .search .uni-searchbar {
  padding: 0;
}
.search-page .history > view {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.search-page .history .title {
  color: #666;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  margin: 10rpx 0;
}
.search-page .history .title .delete {
  width: 40rpx;
  height: 40rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}
.search-page .history .his-item {
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
}
.search-page .history .his-item text {
  display: inline-block;
  height: 30px;
  line-height: 30px;
  padding: 0 12px;
  -webkit-border-radius: 15px;
          border-radius: 15px;
  margin-bottom: 5px;
  margin-right: 7px;
  font-size: 14px;
  color: #6a6a6a;
  border: 1px solid #bb8d4c;
  background-color: #fff;
}
.search-page .search-result {
  padding-bottom: 30rpx;
  overflow-y: scroll;
}
.search-page .search-result .title {
  text-align: center;
  color: #666;
}
.search-page .search-result .sub-list .sub-item {
  padding: 8px 20rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.search-page .search-result .sub-list .sub-item:last-child {
  border-bottom: none;
}

