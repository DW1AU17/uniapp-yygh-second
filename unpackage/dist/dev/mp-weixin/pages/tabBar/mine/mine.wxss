/* 面板相关字体 */
.mine-page {
  padding: 0 30rpx;
  overflow: hidden;
}
.mine-page .mine-header {
  margin-top: 75px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  padding: 26rpx 14rpx;
}
.mine-page .mine-header .head-portrait image, .mine-page .mine-header .head-portrait .head-pic {
  width: 69rpx;
  height: 77rpx;
  margin-right: 14rpx;
  -webkit-transform: scale(1.3);
          transform: scale(1.3);
}
.mine-page .mine-header .head-portrait image.wechat {
  width: 77rpx;
  border: 1px solid #bb8d4c;
}
.mine-page .mine-header .head-portrait .head-pic {
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}
.mine-page .mine-header .head-portrait .head-pic:after {
  border: none;
}
.mine-page .mine-header .login {
  color: #bf9356;
  border: 1px solid #bf9356;
  -webkit-border-radius: 14rpx;
          border-radius: 14rpx;
  padding: 4rpx 12rpx;
  line-height: 1.2;
  font-size: 24rpx;
  margin: -20rpx 0 0 20rpx;
  background: transparent;
}
.mine-page .mine-header .name {
  margin: -8rpx 0 0 18rpx;
}
.mine-page .btn-box {
  position: relative;
}
.mine-page .btn-box .border-box-inner > view {
  font-size: 32rpx;
  color: #000;
  padding: 0 46rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  line-height: 100rpx;
}
.mine-page .btn-box .border-box-inner > view image {
  width: 13rpx;
  height: 22rpx;
}
.mine-page .btn-box .border-box-inner > view:last-child {
  padding-bottom: 0;
}
.mine-page .btn-box .border-box-inner .logout {
  position: absolute;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
  background-color: transparent;
  z-index: 9;
}

