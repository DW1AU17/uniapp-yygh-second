/* 面板相关字体 */
.index-page {
  overflow: hidden;
}
.index-page .main {
  height: calc(100% - 60rpx);
  font-size: 26rpx;
  margin: 30rpx;
}
.index-page .main .border-box-inner {
  height: 100%;
  padding: 0 30rpx;
}
.index-page .main .border-box-inner > view {
  height: 100%;
  overflow-y: auto;
}
.index-page .main .border-box-inner .p {
  margin-bottom: 34rpx;
}
.index-page .main .border-box-inner .p.center {
  text-align: center;
}
.index-page .main .border-box-inner .p > view {
  text-indent: 46rpx;
}
.index-page .main .border-box-inner .p ._img {
  width: 200rpx;
  height: 200rpx;
}
.index-page .main .border-box-inner .p .img {
  width: 100%;
  margin: 0 auto;
}
.index-page .main .border-box-inner .p:nth-child(1) {
  margin-top: 34rpx;
}

