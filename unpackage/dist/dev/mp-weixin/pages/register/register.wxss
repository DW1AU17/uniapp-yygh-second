/* 面板相关字体 */
.reg-page {
  height: 100%;
  padding: 30rpx;
}
.reg-page .cus-form {
  position: relative;
}
.reg-page .cus-form > view {
  padding: 20rpx 0;
}
.reg-page .cus-form .btn-delete {
  position: absolute;
  top: 30rpx;
  right: 32rpx;
  padding: 0 10rpx;
  -webkit-border-radius: 3px;
          border-radius: 3px;
  border: 1px solid #bb8d4c;
  line-height: 1.6;
}
.reg-page .cus-form .cus-form-item {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  font-size: 30rpx;
  padding: 0 50rpx;
  line-height: 70rpx;
}
.reg-page .cus-form .cus-form-item:last-child {
  border-bottom: none;
}
.reg-page .cus-form .cus-form-item .cus-title {
  color: #666;
  width: 170rpx;
  font-weight: 700;
}
.reg-page .remind {
  padding: 20rpx 0;
  font-size: 24rpx;
}
.reg-page .remind .title {
  color: #666;
}
.reg-page .remind .single {
  color: #999;
}
.uni-input-placeholder {
  font-size: 28rpx;
  color: #ccc;
}

