<view class="source-page"><view class="border-box"><view class="border-box-inner"><view class="main"><view class="source-top gradient"><block wx:if="{{!isInit}}"><view class="head-portrait"><view><block wx:if="{{doctorInfo.sffLoginNum==0}}"><image src="/static/ui/0000.jpg" lazy-load="{{true}}"></image></block><block wx:else><image src="{{'https://www.zjgoshine.com:59001/resource/'+doctorInfo.sffLoginNum+'.jpg'}}" data-event-opts="{{[['error',[['handleError',['$event']]]]]}}" binderror="__e"></image></block></view><view class="doc-name">{{doctorInfo.sffName}}</view><view class="pavilion">{{"桐君堂 ("+doctorInfo.pavName+") - "+doctorInfo.sffProfessionalWrapper}}</view><view class="specialty"><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="{{[(summary)?'text-line2':'']}}" bindtap="__e">{{doctorInfo.sffSummary||''}}</view></view></view></block></view><view class="source-list gradient"><block wx:if="{{sourceTab.length}}"><view class="tab"><block wx:for="{{sourceTab}}" wx:for-item="item" wx:for-index="__i0__" wx:key="depId"><view data-event-opts="{{[['tap',[['setCurrentInfo',['$0'],[[['sourceTab','depId',item.depId]]]]]]]}}" class="{{[(item.depId===currentTabIndex)?'active':'']}}" bindtap="__e">{{''+item.depName+''}}</view></block></view></block><view class="table"><view class="thead"><view class="tr"><view class="left"></view><view class="right"><view>上午</view><view>下午</view><view>夜门诊</view></view></view></view><block wx:if="{{sourceListDate.length}}"><view class="tbody"><block wx:for="{{$root.l0}}" wx:for-item="dateItem" wx:for-index="__i1__" wx:key="$orig"><view class="tr"><view class="left"><view class="week">{{dateItem.m0}}</view><view class="date">{{dateItem.m1}}</view></view><view class="right"><block wx:for="{{sourceListDetail[dateItem.$orig]}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view><block wx:if="{{item.state!==1&&item.numremain!=0&&!item.numberReleasedFlag}}"><text data-event-opts="{{[['tap',[['appointment',['$0',index],[[['sourceListDate','',__i1__]]]]]]]}}" class="btn" bindtap="__e">{{"总"+item.numcount+"剩"+item.numremain}}</text></block><block wx:if="{{item.state!==1&&item.numremain!=0&&item.numberReleasedFlag}}"><text>即将放号</text></block><block wx:else><block wx:if="{{item.state!==1&&item.numremain==0}}"><text class="btn disabled">已无号源</text></block></block></view></block></view></view></block></view></block><block wx:else><view class="nothing">{{nothing}}</view></block></view></view><view class="remind"><view class="title">温馨提示</view><view class="single"><view>预约成功后的记录, 可在"个人中心"下"挂号记录"中查询</view></view></view></view><block wx:if="{{visible}}"><view class="custom-picker"><view data-event-opts="{{[['tap',[['e1',['$event']]]]]}}" class="mask" bindtap="__e"></view><view class="footer"><view class="btn"><text data-event-opts="{{[['tap',[['e2',['$event']]]]]}}" class="fc999" bindtap="__e">取消</text><text data-event-opts="{{[['tap',[['goOrderPage',['$event']]]]]}}" class="base-color" bindtap="__e">确定</text></view><picker-view data-event-opts="{{[['change',[['chooseSource',['$event']]]]]}}" bindchange="__e"><picker-view-column><block wx:for="{{getNumList}}" wx:for-item="item" wx:for-index="__i2__" wx:key="id"><view class="item"><text class="mr30">{{item.visitTime}}</text><text>{{item.serialNumber+"号"}}</text></view></block></picker-view-column></picker-view></view></view></block></view></view></view>